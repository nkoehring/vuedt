{
  "name": "vuedt",
  "version": "0.3.6",
  "description": "crazy lightweight Vuejs Date and Time picker component with eyecandy",
  "author": "koehr <n@koehr.in>",
  "main": "./dist/vuedt.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nkoehring/vuedt.git"
  },
  "bugs": {
    "url": "https://github.com/nkoehring/vuedt/issues"
  },
  "homepage": "https://nkoehring.github.io/vuedt",
  "keywords": [
    "vue",
    "datetime",
    "date",
    "time",
    "calendar",
    "datepicker",
    "timepicker",
    "eyecandy"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --formats umd,umd-min --name vuedt src/VueDT.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {},
  "devDependencies": {
    "vue": "^2.6.11",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "vue": {
    "css": {
      "extract": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
